sbt clean universal:packageZipTarball

scp -i "~/.ssh/zinjvi-test.pem" flash-cards-web-app-1.0-SNAPSHOT.tgz   ubuntu@ec2-35-166-20-92.us-west-2.compute.amazonaws.com:

ssh -i "~/.ssh/zinjvi-test.pem" ubuntu@ec2-35-166-20-92.us-west-2.compute.amazonaws.com



####
install java

sudo add-apt-repository ppa:webupd8team/java -y
sudo apt-get update
sudo apt-get install oracle-java8-installer

####
install Postgres

sudo apt-get install postgresql postgresql-contrib
sudo -u postgres psql
create database fc;
ALTER USER "postgres" WITH PASSWORD 'password';


####
Start

nohup ./flash-cards-web-app > out.log &
